{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend-creditos": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap-grid.min.css",
              "src/styles.css"
            ],
            "outputPath": "dist/frontend-creditos/browser"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "550kB",
                  "maximumError": "600kB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "optimization": true,
              "extractLicenses": true,
              "sourceMap": false
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend-creditos:build:production"
            },
            "development": {
              "buildTarget": "frontend-creditos:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:jest",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          },
          "configurations": {
            "ci": {}
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:ssr-dev-server",
          "options": {
            "browserTarget": "frontend-creditos:build",
            "serverTarget": "frontend-creditos:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend-creditos:build:production",
              "serverTarget": "frontend-creditos:server:production"
            },
            "development": {
              "browserTarget": "frontend-creditos:build:development",
              "serverTarget": "frontend-creditos:server:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend-creditos:build:production",
              "serverTarget": "frontend-creditos:server:production"
            },
            "development": {
              "browserTarget": "frontend-creditos:build:development",
              "serverTarget": "frontend-creditos:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}